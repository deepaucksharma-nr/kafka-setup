# New Relic API Configuration
# Get your API keys from: https://one.newrelic.com/api-keys

# Required: Your New Relic User API Key
# This key is used for NerdGraph API calls
# Get it from: https://one.newrelic.com/api-keys
NEW_RELIC_API_KEY=NRAK-XXXXXXXXXXXXXXXXXXXXXXXXX

# Required: Your New Relic Account ID
# Find it in the URL when you're logged into New Relic (e.g., https://one.newrelic.com/accounts/YOUR_ACCOUNT_ID)
NEW_RELIC_ACCOUNT_ID=1234567

# Optional: Query Key for NRQL queries (if different from API key)
# Only needed if you want to use a separate key for query operations
NEW_RELIC_QUERY_KEY=

# Optional: License Key for data ingestion
# Only needed if you're sending custom metrics
NEW_RELIC_LICENSE_KEY=

# Dashboard Configuration
DASHBOARD_NAME=Sample Application Dashboard
DASHBOARD_DESCRIPTION=Monitoring dashboard for sample application

# Optional: Environment settings
NODE_ENV=development
LOG_LEVEL=info

# Optional: API Rate Limiting
API_RATE_LIMIT_PER_MINUTE=60
API_RETRY_ATTEMPTS=3

# Optional: Cache settings (in seconds)
METRIC_CACHE_TTL=300
SCHEMA_CACHE_TTL=3600